        "@babel/preset-env": "^7.9.5",
        "@babel/preset-react": "^7.9.4",
        "axios": "^0.19.2",
        "babel-loader": "^8.1.0",
        "bootstrap": "^4.4.1",
        "compression": "^1.7.4",
        "concurrently": "^5.1.0",
        "express": "^4.17.1",
        "express-http-proxy": "^1.6.0",
        "lodash": "^4.17.15",
        "nodemon": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^5.2.1",
        "react-redux": "^7.2.0",
        "react-router-config": "^5.1.1",
